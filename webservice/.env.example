# FastAPI + React 환경 변수 예시
# 실제 사용 시 .env 파일로 복사하여 사용하세요

# 백엔드 설정
BACKEND_HOST=localhost
BACKEND_PORT=8000
BACKEND_DEBUG=true

# 프론트엔드 설정
FRONTEND_HOST=localhost
FRONTEND_PORT=3000

# API 설정
API_TIMEOUT=30000
API_BASE_URL=http://localhost:8000/api

# Ollama 설정
OLLAMA_HOST=localhost
OLLAMA_PORT=11434
OLLAMA_MODEL=qwen3:8b

# RAG 시스템 설정
RAG_ENABLED=true
VECTOR_DB_PATH=./vector_db_data
EMBEDDING_MODEL=jhgan/ko-sroberta-multitask

# 개발 도구
HOT_RELOAD=true
DEBUG_MODE=true